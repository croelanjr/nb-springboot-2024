<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor name="comment" default="comment"/>
    <fontcolor name="dot" default="operator"/>
    <fontcolor name="array_idx" default="field">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="map_key" default="keyword">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="bracket" default="field"/>
    <fontcolor name="key" default="keyword" />
    <fontcolor name="separator" default="operator"/>
    <fontcolor name="value" default="string" />
    <fontcolor name="whitespace" />
    <fontcolor name="error" default="error"/>
</fontscolors>
